{
    "name": "mikimh/request-filter",
    "description": ":package_description",
    "license": "license",
    "authors": [
        {
            "name": "author name",
            "email": "author email",
            "homepage": "author homepage"
        }
    ],
    "homepage": "https://github.com/mikimh/request-filter",
    "keywords": ["Laravel", "RequestFilter"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Mikimh\\RequestFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mikimh\\RequestFilter\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mikimh\\RequestFilter\\RequestFilterServiceProvider"
            ],
            "aliases": {
                "RequestFilter": "Mikimh\\RequestFilter\\Facades\\RequestFilter"
            }
        }
    }
}
